{
  "compilerOptions": {
    "esModuleInterop": true,
    "strict": true,
    "lib": ["ES2021"],
    "module": "ES2020",
    "target": "ES2021",
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "Node",
    "sourceMap": true,
    "rootDir": "./src",
    "outDir": "./dist",
    "incremental": true,
  }
}
